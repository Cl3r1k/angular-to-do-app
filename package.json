{
    "name": "angular-to-do-app",
    "version": "1.4.4",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Cl3r1k/angular-to-do-app.git"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "json-server": "json-server --watch db.json",
        "json-server-auth": "node json-server.js",
        "app": "concurrently \"node json-server.js\" \"node ./replace.build.js\" \"npm run start\"",
        "app-old": "concurrently \"node ./replace.build.js\" \"npm run start\"",
        "app-server": "concurrently \"npm run json-server\" \"node ./replace.build.js\" \"npm run start\"",
        "update-patch": "npm --no-git-tag-version version patch",
        "deploy-gh": "ng build --prod --base-href 'https://Cl3r1k.github.io/angular-to-do-app/' && ngh",
        "sw": "ng build --prod && http-server -c-1 -p 4200 dist/"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^6.1.1",
        "@angular/cdk": "^6.0.0",
        "@angular/common": "^6.1.1",
        "@angular/compiler": "^6.1.1",
        "@angular/core": "^6.1.1",
        "@angular/forms": "^6.1.1",
        "@angular/http": "^6.1.1",
        "@angular/material": "^6.0.0",
        "@angular/platform-browser": "^6.1.1",
        "@angular/platform-browser-dynamic": "^6.1.1",
        "@angular/platform-server": "^6.1.1",
        "@angular/router": "^6.1.1",
        "@beyerleinf/ngx-dnd": "^6.0.0-preview3",
        "body-parser": "^1.18.3",
        "core-js": "^2.5.3",
        "dexie": "^2.0.1",
        "g": "^2.0.1",
        "hammerjs": "^2.0.8",
        "json-server": "^0.12.1",
        "ng-autosize": "^1.1.0",
        "rxjs": "^6.2.2",
        "zone.js": "^0.8.20"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.7.0",
        "@angular/cli": "^6.1.2",
        "@angular/compiler-cli": "^6.1.1",
        "@angular/language-service": "^6.1.1",
        "@types/jasmine": "^2.8.6",
        "@types/jasminewd2": "^2.0.3",
        "@types/node": "^6.0.96",
        "codelyzer": "^4.1.0",
        "concurrently": "^4.0.1",
        "jasmine-core": "^2.9.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "^3.0.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.4.1",
        "karma-jasmine": "~1.1.1",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "^5.1.2",
        "replace-in-file": "^3.1.0",
        "ts-node": "~4.1.0",
        "tslint": "~5.9.1",
        "typescript": "~2.9.2"
    }
}
